{
  "name": "Brizer-crx",
  "description": "Brizer's tool",
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Brizer's tool",
    "default_popup": "ui/popup/index.html",
    "default_icon": {
      "19": "icons/kt19.png",
      "38": "icons/kt38.png"
    }
  },
  "background": {
    "persistent": true,
    "page": "background/index.html"
  },
  "permissions": ["storage", "tabs", "<all_urls>"]
}
